{
  "parser": "babel-eslint",

    "env": {
        "browser": true,
        "node": true
    },

    "globals": {
        "React": true
    },

  "parserOptions": {
    "ecmaFeatures": {
        "jsx": true,
        "modules": true
    },
    "ecmaVersion": 7,
    "sourceType": "module"
  },

    "plugins": [
        "react"
    ]
}